{
  "name": "wallaby-tsx",
  "version": "0.5.0",
  "description": "Small example using Wallaby with TypeScript/Jest w/ tsx",
  "main": "index.js",
  "author": "Hajime Yamasaki Vukelic",
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "cross-env NODE_ENV=production webpack",
    "start": "webpack-dev-server -d"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/node": "8.0.26",
    "@types/webpack-env": "^1.13.0",
    "@types/whatwg-fetch": "0.0.33",
    "autoprefixer": "^6.7.7",
    "bundle-loader": "^0.5.5",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "^0.11.2",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.6.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jed-gettext-parser": "^1.0.0",
    "jest": "^20.0.1",
    "jest-fetch-mock": "^1.1.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "progress-bar-webpack-plugin": "^1.9.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "ts-jest": "^20.0.14",
    "ts-loader": "^2.0.3",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.2",
    "vue-property-decorator": "^5.0.1",
    "vuex": "^2.4.0",
    "webfonts-loader": "^1.2.0",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "es5-shim": "^4.5.9",
    "es6-promise": "^4.1.1",
    "es6-shim": "^0.35.3",
    "vue": "^2.3.3",
    "vuex-class": "^0.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.test\\.tsx?$",
    "modulePaths": [
      "<rootDir>/src/",
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "(\\.scss)": "identity-obj-proxy"
    },
    "mapCoverage": true
  }
}
